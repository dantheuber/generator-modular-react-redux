{
  "name": "<%= name %>",
  "version": "1.4.0",
  "description": "A boilerplate modular react-redux application",
  "main": "index.js",
  "scripts": {
    "start": "bash ./scripts/run-dev",
    "dev": "bash ./scripts/dev-webpack",
    "build": "bash ./scripts/build",
    "clean": "bash ./scripts/clean",
    "test": "bash ./scripts/run-tests",
    "lint": "node_modules/.bin/eslint --ext js --ext jsx --fix src/ config/ tests/",
    "watch-lint": "node_modules/.bin/esw -w --ext js --ext jsx --fix src/ config/ tests/"
  },
  "repository": "<%= repo %>",
  "keywords": [
    "react",
    "redux"
  ],
  "author": "<%= author %>",
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/containers/*.{js,jsx}",
      "!src/**/constants.js",
      "!src/**/index.js",
      "!src/vendor.js",
      "!src/actions.js",
      "!src/reducers.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "roots": [
      "src/"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupTestFrameworkScriptFile": "./tests/setup-jest",
    "testResultsProcessor": "./node_modules/jest-junit"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "babel-plugin-transform-builtin-extend",
        {
          "globals": [
            "Error"
          ]
        }
      ],
      "transform-object-rest-spread",
      "babel-plugin-syntax-dynamic-import"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^7.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react-app": "^3.1.1",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-jest": "^2.1.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^5.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.1",
    "fs-extra": "^3.0.1",
    "handlebars": "^4.5.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.0",
    "jest-environment-node-debug": "^2.0.0",
    "jest-junit": "^6.3.0",
    "jscs-jsx-rules": "0.0.1",
    "mock-local-storage": "^1.1.12",
    "postcss-loader": "^1.3.3",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "style-ext-html-webpack-plugin": "^3.4.7",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-version-file": "^0.1.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash.clone": "^4.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-pure-lifecycle": "^2.2.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-little-router": "^15.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  }
}
